{
    "routes": [
      {
        "src": "/__/auth/(.*)",
        "dest": "/signin_helpers/$1"
      },
      {
        "src": "/__/firebase/init.json",
        "dest": "/signin_helpers/init.json"
      }, 
      {
        "src": "/createNewPassword(?:\\?.*)?$",
        "dest": "/index.html"
      },
      {
        "src": "/account-registered(?:\\?.*)?$",
        "dest": "/index.html"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  